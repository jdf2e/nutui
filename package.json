    "npm-run-all": "^4.1.5",
    "nyc": "10.0.0",
    "offline-plugin": "^5.0.6",
    "optimize-css-assets-webpack-plugin": "5.0.0",
    "ora": "3.0.0",
    "path": "^0.12.7",
    "portfinder": "1.0.17",
    "postcss-import": "12.0.0",
    "postcss-loader": "3.0.0",
    "postcss-url": "8.0.0",
    "qrcode": "1.3.2",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "sass-resources-loader": "1.3.3",
    "semver": "5.5.1",
    "shelljs": "0.8.2",
    "style-loader": "0.22.1",
    "svg-sprite-loader": "3.9.2",
    "three": "^0.99.0",
    "uglifyjs-webpack-plugin": "1.3.0",
    "url-loader": "1.1.1",
    "vue": "2.5.17",
    "vue-i18n": "8.1.0",
    "vue-jest": "2.6.0",
    "vue-loader": "15.4.0",
    "vue-router": "^3.0.2",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.5.17",
    "vueg": "1.3.4",
    "webpack": "4.25.0",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.11",
    "webpack-merge": "4.1.4",
    "webpack-node-externals": "1.7.2"
  },
  "browserslist": [
    "> 3%",
    "Android >= 4",
    "iOS >= 8"
  ],
  "nyc": {
    "include": [
      "src/packages/**/*.vue"
    ],
    "instrument": false,
    "sourceMap": false
  }
}
